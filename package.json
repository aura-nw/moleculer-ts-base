{
  "name": "lang-feature",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "_moduleAliases": {
    "@base": "src/base"
  },
  "scripts": {
    "clean": "rimraf ./dist/",
    "start": "cd dist ;  node --es-module-specifier-resolution=node ../node_modules/moleculer/bin/moleculer-runner.mjs --env --config ./moleculer.config.js",
    "dev": "ts-node -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.mjs --hot --repl --env --config src/moleculer.config.ts",
    "unused:mole1": "ts-node -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --env --config src/moleculer.config.ts",
    "unused:mole2": "ts-node ./node_modules/moleculer/bin/moleculer-runner.mjs --env --config src/moleculer.config.ts",
    "build": "yarn clean; tsc --project tsconfig.build.json",
    "prepare": "husky install",
    "test": "jest --config jest.config.js",
    "lint": "echo 'Running lint for repository'",
    "test": "echo Running unit test"
  },
  "dependencies": {
    "bull": "^4.10.2",
    "dotenv": "^16.0.3",
    "ioredis": "^5.2.4",
    "lodash-es": "^4.17.21",
    "moleculer": "^0.14.21",
    "moleculer-bull": "^0.3.1",
    "moleculer-decorators": "^1.3.0",
    "reflect-metadata": "^0.1.13",
    "util": "^0.12.4"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.3.0",
    "@tsconfig/node16": "^1.0.3",
    "@types/node": "^18.11.11",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.0",
    "moleculer-repl": "^0.7.3",
    "prettier": "^2.7.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.8.4"
  }
}
